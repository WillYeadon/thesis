FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4850";
    object      UxBS;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    hot
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    hotTop
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    cold
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    coldTop
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    top
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    btm
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


