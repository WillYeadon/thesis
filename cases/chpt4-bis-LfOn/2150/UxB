FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "2150";
    object      UxB;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 0 0 -1 0];

internalField   uniform (0 0 0);

boundaryField
{
    hot
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    hotTop
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    cold
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    coldTop
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    top
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    btm
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


