FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    object      A;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -2 0 0 -1 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
      type   zeroGradient;
    }
    btm
    {
      type   zeroGradient;
    }
    sym
    {
        type            symmetry;
    }
    back
    {
        type            wedge;
    }
    wallTube
    {
        $btm;
    }
    front
    {
        type            wedge;
    }
}           

