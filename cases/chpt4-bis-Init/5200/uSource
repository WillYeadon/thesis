FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "5200";
    object      uSource;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    hot
    {
        type            zeroGradient;
    }
    hotTop
    {
        type            zeroGradient;
    }
    cold
    {
        type            zeroGradient;
    }
    coldTop
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    btm
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


