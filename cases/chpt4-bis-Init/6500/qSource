FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "6500";
    object      qSource;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    hot
    {
        type            zeroGradient;
    }
    hotTop
    {
        type            zeroGradient;
    }
    cold
    {
        type            zeroGradient;
    }
    coldTop
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    btm
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


